#!/bin/sh /etc/rc.common
# Copyright (C) 2010-2011 OpenWrt.org

START=80

USE_PROCD=1

start_service() {
    procd_open_instance
    procd_set_param command /usr/bin/redis -f /etc/redis.conf
    procd_set_param respawn
    procd_set_param stdout 0
    procd_set_param stderr 0
    procd_close_instance
}
